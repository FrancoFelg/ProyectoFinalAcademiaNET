@{
}
<body>
<div class="container justify-items-center">
    <h2>lista de Clientes </h2>
    <div class="mb-2">
        <a asp-action="RegistroProv" asp-controller="Clientes" class="btn btn-outline-info">Registrar </a>
    </div>

    <div>
 <table class="table table-dark table-striped-columns g-6">
                @foreach (var item in Model)
                {


               <tr>
                    <th>@item.nombre</th>
                    <th>@item.apellido_razsoc</th>
                    <th>@item.doc_tipo_id</th>
                    <th>@item.doc_nro</th>

                        <th>  <a asp-action="ModificarProv" asp-controller="Clientes" class="btn btn-outline-primary" asp-route-id="@item.Id">Modificar</a></th>
                        <th>  <a asp-action="EliminarProv" asp-controller="Clientes" class="btn btn-outline-danger" asp-route-id="@item.Id">eliminar</a></th>
               </tr>



                }
 </table>
 </div>
 </div>
</body> 
